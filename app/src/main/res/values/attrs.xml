<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <declare-styleable name="AnimationView">
        <attr name="source" format="string" />
        <attr name="type" format="enum">
            <enum name="GIF" value="0" />
            <enum name="SVGA" value="1" />
            <enum name="PAG" value="2" />
            <enum name="VAP" value="3" />
        </attr>
        <attr name="loopCount" format="integer" />
        <attr name="clearsAfterDetached" format="boolean" />
    </declare-styleable>
</resources>